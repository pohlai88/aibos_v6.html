{
  "name": "aibos-v6-saas",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "doc-check": "node scripts/doc-sync.cjs",
    "doc-validate": "node scripts/doc-sync.cjs validate",
    "doc-report": "node scripts/doc-sync.cjs report",
    "setup-hooks": "chmod +x scripts/pre-commit-hook.sh && cp scripts/pre-commit-hook.sh .git/hooks/pre-commit",
    "pre-push-check": "node scripts/pre-push-validation.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@supabase/supabase-js": "^2.38.4",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.0.0"
  }
}

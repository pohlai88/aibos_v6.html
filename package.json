{
  "name": "aibos-v6-saas",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "doc-check": "node scripts/doc-sync.cjs",
    "doc-validate": "node scripts/doc-sync.cjs validate",
    "doc-report": "node scripts/doc-sync.cjs report",
    "setup-hooks": "chmod +x scripts/pre-commit-hook.sh && cp scripts/pre-commit-hook.sh .git/hooks/pre-commit",
    "pre-push-check": "node scripts/pre-push-validation.cjs",
    "verify-phase2": "node scripts/verify-phase2.cjs",
    "verify-phase2:security": "node scripts/verify-phase2.cjs --security=full",
    "verify-phase2:performance": "node scripts/verify-phase2.cjs --performance",
    "verify-phase2:types": "node scripts/verify-phase2.cjs --types=strict",
    "verify-phase2:all": "node scripts/verify-phase2.cjs --all",
    "ts-hotfix": "node scripts/ts-hotfix.cjs",
    "monitor": "node scripts/monitor-phase2.cjs",
    "test:phase2": "node scripts/test-phase2.cjs",
    "verify-merge-readiness": "node scripts/verify-merge-readiness.cjs",
    "verify-production-readiness": "node scripts/verify-production-readiness.cjs",
    "deploy:staging": "node scripts/deploy-staging.cjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run --reporter=verbose tests/integration/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:security": "vitest run --reporter=verbose tests/security/**/*.test.{ts,tsx}",
    "test:performance": "vitest run --reporter=verbose tests/performance/**/*.test.{ts,tsx}",
    "test:mfrs": "vitest run --reporter=verbose tests/mfrs/**/*.test.{ts,tsx}",
    "test:functional": "vitest run --reporter=verbose tests/functional/**/*.test.{ts,tsx}",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:performance && npm run test:mfrs && npm run test:functional",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:debug": "vitest --inspect-brk",
    "test:update": "vitest -u",
    "test:report": "vitest run --reporter=html --reporter=json",
    "test:validate": "node scripts/validate-test-coverage.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^5.1.1",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.81.5",
    "@types/d3": "^7.4.3",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.0",
    "fuse.js": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "moment-timezone": "^0.6.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-router-dom": "^7.6.3",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.72"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.0.0",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/reporter-html": "^2.1.8",
    "@vitest/reporter-json": "^2.1.8",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/react-hooks": "^8.0.1",
    "jsdom": "^25.0.1",
    "vitest": "^2.1.8",
    "playwright": "^1.48.0",
    "@playwright/test": "^1.48.0",
    "happy-dom": "^14.10.1",
    "msw": "^2.3.1",
    "faker": "^6.6.6",
    "@faker-js/faker": "^9.4.0"
  }
}
